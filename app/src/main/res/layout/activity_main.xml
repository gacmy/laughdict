<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
   <FrameLayout
       android:id="@+id/main_container"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:layout_above="@+id/maintab"/>

   <com.gac.laughdict.widget.bottombar.MainNavigateTabBar
       android:id="@+id/maintab"
       android:layout_alignParentBottom="true"
       android:background="@color/colorPrimary"
       app:containerId="@+id/main_container"
       app:navigateTabSelectedTextColor="@color/colorAccent"
       app:navigateTabTextColor="@color/textcolor35"
       android:layout_width="match_parent"
       android:layout_height="wrap_content">


   </com.gac.laughdict.widget.bottombar.MainNavigateTabBar>
</RelativeLayout>
